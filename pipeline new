node 
 {
    def mavenHome = tool name: "maven3.6.3"
    stage('GITclone') 
    
    {
        git branch: 'development', url: 'https://github.com/Assam2342/maven-web-application.git'
    }
     stage('Build')
 {
  sh "${mavenHome}/bin/mvn clean package"
 }
 stage('tomcat deploy')
 sshagent(['ubuntu']) 
 {
    sh "scp -o StrictHostKeyChecking=no target/maven-web-application.war ubuntu@3.142.73.94:/opt/apache-tomcat-9.0.53/webapps/"
}
}
